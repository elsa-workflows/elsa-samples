@page "/workflow/definitions"
@layout ElsaStudioLayout

<PageTitle>Workflow Definitions</PageTitle>
<WorkflowDefinitionList EditWorkflowDefinition="@OnEditWorkflowDefinition"/>

@code
{
    [Inject] private NavigationManager NavigationManager { get; set; } = null!;

    private void OnEditWorkflowDefinition(string definitionId)
    {
        NavigationManager.NavigateTo($"/workflow/definitions/{definitionId}");
    }
}
